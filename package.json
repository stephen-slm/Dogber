{
  "name": "dogber",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && firebase deploy",
    "test": "npm run test:unit",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest -i --config ./jest.config.js",
    "test:unit": "vue-cli-service test:unit",
    "precommit": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "firebase": "^5.8.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "vue": "^2.6.7",
    "vue-router": "^3.0.2",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^1.5.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.7"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 110,
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
